uzu devel/TAP :all 
uzu awk
uzu ldap

got= expected=
lf=ldap/filter

test/awk () {
    awk/new lol go to hell -- -F'[\\\\ ]'
    got=$( lol '{ print $to }' <<< 'go to\hell' )
    is $got to "awk/new works"
}

test/ldap/filter () {
    got=$( $lf/minify <<< "
        (| (uid=a)
           (uid=b))" )
    expected="(|(uid=a)(uid=b))"
    is $got $expected "ldap minification works" ||
        note "got $got"
}

suite () {
    test/awk 
    test/ldap/filter
}

prove suite
