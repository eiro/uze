# TODO: document

shush1   () { "$@" 1> /dev/null }
shush2   () { "$@" 2> /dev/null }
shush    () { "$@" &> /dev/null }
warn     () { local r=$?; print -u2 "$*"; return $r }
die      () { local r=$?; print -u2 "$*"; exit $r   }
slurp    () { IFS=$'\n' read -d '' -A $1 }
fill     () { local garbage; IFS=$'\n' read -d '' "$@" garbage }

